{
	"settings": {
		"lemonbar": "lemonbar -U #0A0A0A -u 4 -B #0A0A0A -g x24 -p",
		"font": "Iosevka Nerd Font"
	},

	"template": {
		"left": ["cpu", "ram"],
		"center": ["date", "time"],
		"right": ["workspaces", "power"]
	},

	"blocks": [
		{
			"name": "cpu",
			"module": "cpu", 
			"template": "CPU: <cpu-percent-used>",
			"interval": "1"
		},
		{
			"name": "ram",
			"module": "ram",
			"interval": "1",
			"template": "RAM: <ram-percent-used>"
		},
		{
			"name": "workspaces",
			"command": "./workspaces",
			"template": "<output>",
			"interval": "ondata"
		}
	],

	"buttons": [
		{
			"name": "power",
			"onclick": "rofi ...",
			"label": "Power"
		}
	]
}
